{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": []
  },
  "targetDefaults": {
    "@angular-devkit/build-angular:browser": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    }
  },
  "generators": {
    "@nx/angular:application": {
      "e2eTestRunner": "none",
      "style": "css",
      "unitTestRunner": "none",
      "standalone": false
    },
    "@nx/angular:component": {
      "style": "css",
      "type": "component",
      "skipTests": true,
      "standalone": false
    },
    "@nx/angular:directive": {
      "type": "directive",
      "skipTests": true,
      "standalone": false
    },
    "@nx/angular:pipe": {
      "skipTests": true,
      "standalone": false
    },
    "@nx/angular:service": {
      "type": "service",
      "skipTests": true
    },
    "@nx/angular:class": {
      "skipTests": true
    },
    "@nx/angular:guard": {
      "skipTests": true
    },
    "@nx/angular:interceptor": {
      "skipTests": true
    }
  },
  "plugins": [
    {
      "plugin": "@nx/webpack/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "buildDepsTargetName": "build-deps",
        "watchDepsTargetName": "watch-deps"
      }
    }
  ]
}
